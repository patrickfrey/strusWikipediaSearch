TERM sentence "":sent;

SELECT selfeat;
EVAL 1 * bm25_dpfc( k1=0.75, b=2.1, avgdoclen=1000, relevant=0.1, .match=docfeat, .title=title, doclen_title=doclen_tist, .struct=sentence);
EVAL 2 * metadata( name=pageweight);

SUMMARIZE title = attribute( name=title );
SUMMARIZE date = attribute( name=date );
SUMMARIZE contributor = attribute( name=contributor );
SUMMARIZE summary = matchphrase( type=orig, nof=4, len=60, structseek=40, .struct=sentence, .match=sumfeat );

