[ForwardIndex]
    doc      = empty content /mediawiki/page;
    docid    = lc:wordjoin("_") content /mediawiki/page/title();
    start    = empty content /mediawiki/page/revision/text;
    end      = empty content /mediawiki/page/revision/text~;
    linkid   = lc:wordjoin("_") content /mediawiki/page/revision/text//link[@type='page']@id;
    redirect = const("->") content /mediawiki/page/revision/text/redirect;


