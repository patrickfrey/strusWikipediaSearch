    lexem = orig punctuation(en,".") /mediawiki/page/revision/text();
    lexem = orig content /mediawiki/page/revision/text/l1~;
    lexem = orig content /mediawiki/page/revision/text/l2~;
    lexem = orig content /mediawiki/page/revision/text/l3~;
    lexem = orig content /mediawiki/page/revision/text/l4~;
    lexem = orig content /mediawiki/page/revision/text/h1~;
    lexem = orig content /mediawiki/page/revision/text/h2~;
    lexem = orig content /mediawiki/page/revision/text/h3~;
    lexem = orig content /mediawiki/page/revision/text/h4~;
    lexem = orig content /mediawiki/page/revision/text/h5~;
    lexem = orig content /mediawiki/page/revision/text/h6~;
    lexem = orig content /mediawiki/page/revision/text//link[@type='file']~;
    lexem = orig content /mediawiki/page/revision/text//link[@type='image']~;
    lexem = orig content /mediawiki/page/revision/text//link[@type='category']~;
    lexem = orig content /mediawiki/page/revision/text//li~;
    lexem = orig content /mediawiki/page/revision/text//td~;

[PatternMatch std]
    vecfeat  = pattern_searchfeat_doc.txt;
    vecname  = pattern_forwardfeat_doc.txt;

[SearchIndex]
    vecfeat <- vecfeat;

[ForwardIndex]
    vecname <- vecname;


