[PatternMatch std]
    vectfeat  = pattern_titlefeat_doc.txt;
    veclfeat  = pattern_lnkfeat_doc.txt;
    vecsfeat  = pattern_searchfeat_doc.txt;
    vecfname  = pattern_forwardfeat_doc.txt;

[SearchIndex]
    vecsfeat <- vecsfeat;
    vectfeat <- vectfeat;

[ForwardIndex]
    vecfname <- vecfname;
    veclfeat <- veclfeat;


