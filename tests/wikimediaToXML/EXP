    <pagelink id="Midfielder">Midfielder
      </pagelink>
    </attr>
  <attr id="youthyears1"/>
  <attr id="youthclubs1"/>
  
  <attr id="clubs1"><text>  </text>
    <pagelink id="Croydon Kings">Croydon Kings
      </pagelink>
    <text>  </text>
    </attr>
  
  
  
  <attr id="clubs2"><text>  </text>
    <pagelink id="Adelaide Comets">Adelaide Comets
      </pagelink>
