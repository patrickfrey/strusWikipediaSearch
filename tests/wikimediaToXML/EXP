## 0000/Galeruca.xml
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<doc><docid>Galeruca</docid>
<title>Galeruca</title>
<citlink id="cit1"/>
<text>
</text>
<entity><quot><text>Galeruca</text>
    <entity><quot><text> is a genus of leaf beetle. In </text>
        <pagelink id="Turkey">Turkey
          </pagelink>
        <text>, the genus is represented by 10 species.</text>
        </quot>
      </entity>
    </quot>
  </entity>
<citlink id="cit2"/>
<reflink id="ref2"/>
<ref id="ref2"><text>Aslan, I., Warchalowski, A. &amp; Özbek, H. 2000. A Preliminary
Review of the Subfamily Galerucinae (Coleoptera, Chrysomelidae) in Turkey. Journal of the Entomological
Research Society 2: 27-42</text>
  </ref>
<citation id="cit1" class="Taxobox" image="Galeruca pomonae side.jpg"><attr id="image_caption"><text> </text>
    <pagelink id="Galeruca pomonae">Galeruca pomonae
      </pagelink>
    </attr>
  <attr id="regnum"><text> </text>
    <pagelink id="Animal">Animalia
      </pagelink>
    <text/>
    </attr>
  <attr id="phylum"><text> </text>
    <pagelink id="Arthropod">Arthropoda
      </pagelink>
    <text/>
    </attr>
  <attr id="classis"><text> </text>
    <pagelink id="Insect">Insecta
      </pagelink>
    <text/>
    </attr>
  <attr id="ordo"><text> </text>
