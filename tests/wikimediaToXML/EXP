<pagelink id="Category:Manlii"><text>Vulso, Gnaeus</text>
  </pagelink>
<citation id="cit1" class="reflist"/>
<citation id="cit2" class="SmithDGRBM" title="Vulso" volume="3" page="1285"/>
<citation id="cit3" class="s-start"/>
<citation id="cit4" class="s-off"/>
<citation id="cit5" class="s-bef"><attr id="before"><pagelink id="Publius Valerius Poplicola (consul 475 BC)"><text>Publius Valerius Poplicola</text>
      </pagelink>
    <text>, and </text>
    <pagelink id="Gaius Nautius Rutilus">Gaius Nautius Rutilus
      </pagelink>
    </attr>
  </citation>
<citation id="cit6" class="s-ttl"><attr id="title"><pagelink id="List of Roman Republican consuls"><text>Consul</text>
      </pagelink>
    <text> of the </text>
    <pagelink id="Roman Republic">Roman Republic
      </pagelink>
    </attr>
  <attr id="years">474 BC</attr>
  <attr id="regent1"><pagelink id="Lucius Furius Medullinus (consul 474 BC)"><text>Lucius Furius Medullinus Fusus</text>
      </pagelink>
    </attr>
  </citation>
<citation id="cit7" class="s-aft"><attr id="after"><pagelink id="Lucius Aemilius Mamercus">Lucius Aemilius Mamercus
      </pagelink>
    <text>, and </text>
    <pagelink id="Vopiscus Julius Iulus">Vopiscus Julius Iulus
      </pagelink>
    </attr>
  </citation>
<citation id="cit8" class="s-end"/>
