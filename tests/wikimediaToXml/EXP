      <entity><quot><text>Vriesea friburgensis</text>
          <entity><quot><text> is a </text>
              <pagelink id="species (biology)"><text>species</text>
                </pagelink>
              <text> in the </text>
              <pagelink id="genus (biology)"><text>genus</text>
                </pagelink>
              <text> </text>
              </quot>
            <pagelink id="Vriesea">Vriesea
              </pagelink>
