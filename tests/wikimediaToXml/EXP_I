    <head id="C3"><text>publisher</text>
      </head>
    <cell id="C3"><pagelink id="GameSpot">GameSpot
        </pagelink>
      <text> </text>
      </cell>
    <head id="C4"><text>url</text>
      </head>
    <cell id="C4"><weblink id="http://www.gamespot.com/mobile/sports/bushvskerryboxing/review.html"/>
