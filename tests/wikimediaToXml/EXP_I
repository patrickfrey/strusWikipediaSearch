  <text>, 1905) </text>
  </list>
<list lv="l1"><text> </text>
  <entity lv="2"><pagelink id="Tambja anayana">Tambja anayana
      </pagelink>
    </entity>
  <text> Ortea, 1989 </text>
  </list>
<list lv="l1"><text> </text>
