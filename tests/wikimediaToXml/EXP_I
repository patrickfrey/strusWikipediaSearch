<citation id="cit17"><table id="table12"><tabtitle><text>Racism topics</text>
      </tabtitle>
    <head id="C0"><text>state</text>
      </head>
    <cell id="C0"><text>autocollapse</text>
      </cell>
    </table>
  </citation></doc>


